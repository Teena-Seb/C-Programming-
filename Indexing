//Write a C program to find the index position of a given character in a string.

#include<stdio.h>
#include<string.h>
int main(){
    char ster[100],w;
    int n,i;
    fgets(ster,100,stdin);
    scanf("%c", &w);
    n=strlen(ster);
    for(i=0; i<n; i++){
        if(ster[i]==w)
            break;
    }
    printf("%d",i+1);
    return 0;
}
