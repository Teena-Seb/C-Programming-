//Write a C program to check whether the given input is prime or not prime.

#include<stdio.h>

int main(){
    int temp=0;
    int num;
    int bol=0;
    scanf("%d",&num);
    for (temp=1; temp<=num; temp++)
        if(num%temp==0)
            bol++;
    if(bol==2)
        printf("prime");
    else
        printf("not prime");
    return 0;
}
